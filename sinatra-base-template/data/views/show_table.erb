<h4>Table Elements</h4>
<table border = "1" style="color:#FF4040;">
<%@rs4.each do |table|%>
<%@t = table.each_slice(1).to_a%>
<%n=@t.size%>
<tr>
<%for i in 0..n%>
<td><%=@t[i]%></td>
<%end%>
</tr>
<%end%>
</table>

